{
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
        "src": "public/**",
        "use": "@vercel/static"
    }
  ],
  "routes":[
    {
        "src":"/(.*)",
        "dest":"/"
    },
    {
        "src": "/(.+)",
        "dest": "public/$1"
    },
    {
        "src": "/",
        "dest": "public/index.html"
    }
  ]
}
